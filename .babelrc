{
	"presets": ["module:metro-react-native-babel-preset"],
	"plugins": [
		[
			"module-resolver",
			{
				"root": ["./src"],
				"extensions": [
					".ios.js",
					".android.js",
					".js",
					".ts",
					".jsx",
					".tsx",
					".json"
				],
				"alias": {
					"tests": ["./__tests__/"]
				}
			}
		]
	]
}
